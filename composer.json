{
    "name": "localgovdrupal/localgov_microsites",
    "description": "LocalGov Drupal Microsites distribution",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "homepage": "https://github.com/localgovdrupal/localgov_microsites",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.4",
        "drupal/admin_toolbar": "^3.1",
        "drupal/autosave_form": "^1.3",
        "drupal/core": "^9.3",
        "drupal/default_content": "^2.0@alpha",
        "drupal/domain_group": "3.x-dev",
        "drupal/domain_path": "1.x-dev#33fde0b",
        "drupal/gin": "^3.0@beta",
        "drupal/gin_login": "^1.0@RC",
        "drupal/gin_toolbar": "^1.0@beta",
        "drupal/metatag": "^1.22",
        "localgovdrupal/localgov_blogs": "^1.0@alpha",
        "localgovdrupal/localgov_core": "^2.1.0",
        "localgovdrupal/localgov_directories": "^2.2",
        "localgovdrupal/localgov_events": "^2.0",
        "localgovdrupal/localgov_microsites_group": "^1.0@alpha",
        "localgovdrupal/localgov_microsites_base": "^1.0@alpha",
        "localgovdrupal/localgov_microsites_colour_picker_fields": "^1.0@alpha",
        "localgovdrupal/localgov_news": "^2.1",
        "localgovdrupal/localgov_page": "^1.0@beta",
        "localgovdrupal/localgov_claro": "^2.0.0",
        "localgovdrupal/localgov_sa11y": "^1.0@alpha"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
	    "drupal/domain_path": {
                "https://github.com/localgovdrupal/localgov_microsites/pull/175#issuecomment-1172879196": "https://raw.githubusercontent.com/localgovdrupal/localgov_microsites/995261d0909065e3124fba3fc0dce3e823aefa1d/patches/domain_path.146-url-aliases.patch"
	    }
	}
    }
}
